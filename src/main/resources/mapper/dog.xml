<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">


<mapper namespace="Dog">
    <insert id="save" parameterType="dog">
        insert into dog_table(m_id,dogName,dogDateBirth,dogDayWeMet,dogGender,dogProfileName,dogContents)
        values(#{m_id},#{dogName},#{dogDateBirth},#{dogDayWeMet},#{dogGender},#{dogProfileName},#{dogContents})
    </insert>
</mapper>